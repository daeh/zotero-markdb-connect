{
  "extends": "./tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "sourceMap": false,
    "removeComments": true,
    // "noUnusedLocals": false,
    // "noUnusedParameters": false,
    // "noImplicitAny": false,
    // "noFallthroughCasesInSwitch": false
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "importHelpers": true,
    "disableSizeLimit": true,
    // "noImplicitAny": false,
    "preserveConstEnums": true,
    "downlevelIteration": true
  }
}
