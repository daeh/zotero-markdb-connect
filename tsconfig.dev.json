{
  "extends": "./tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "removeComments": false,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": false,
    //
    "allowUnreachableCode": false
    ///
  }
}
